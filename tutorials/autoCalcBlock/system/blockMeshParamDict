{
    minX            -3;
    minY            -3;
    minZ            -3;
    maxX            3;
    maxY            3;
    maxZ            3;
}

solidBoundingBox
{
    minX            -1;
    minY            -1;
    minZ            -1;
    maxX            1;
    maxY            1;
    maxZ            1;
}

