{
    temperature     293.15;
    gravity         (0. 0. -9.81);
    inlet
    {
        velocity        (0. 0. 0.);
        temperature     293.15;
    }
}

solids
{
    solid1
    {
        conductivity 210.0;
        elementLength 0.001;
        emissivity 0.8;
        power 0;
    }

    solid2
    {
        conductivity 210.0;
        elementLength 0.001;
        emissivity 0.8;
        power 0;
    }
}


