edgeLength
{
    minX            #calc "int(($:solidBoundingBox.minX - $:fluidBoundingBox.minX)/$:domain.elementLength)";
    minY            #calc "int(($:solidBoundingBox.minY - $:fluidBoundingBox.minY)/$:domain.elementLength)";
    minZ            #calc "int(($:solidBoundingBox.minZ - $:fluidBoundingBox.minZ)/$:domain.elementLength)";
    midX            #calc "int(($:solidBoundingBox.maxX - $:solidBoundingBox.minX)/$:domain.elementLength)";
    midY            #calc "int(($:solidBoundingBox.maxY - $:solidBoundingBox.minY)/$:domain.elementLength)";
    midZ            #calc "int(($:solidBoundingBox.maxZ - $:solidBoundingBox.minZ)/$:domain.elementLength)";
    maxX            #calc "int(($:fluidBoundingBox.maxX - $:solidBoundingBox.maxX)/$:domain.elementLength)";
    maxY            #calc "int(($:fluidBoundingBox.maxY - $:solidBoundingBox.maxY)/$:domain.elementLength)";
    maxZ            #calc "int(($:fluidBoundingBox.maxZ - $:solidBoundingBox.maxZ)/$:domain.elementLength)";
}

