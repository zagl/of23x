FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "./system/includeDict"

dictionaryReplacement
{
    boundary
    {
        "oldInternalFaces"
        {
            type            wall;
        }
        ".*_to_F.*"
        {
            inGroups        ($:config.changeDictionary.coupledWallType);
        }
        ".*_to_[^F].*"
        {
            inGroups        (coupledWall);
        }

        $:config.changeDictionary.regions.SOLID
    }
}

#remove config;

// ************************************************************************* //
